{
    "compilerOptions": {
        "esModuleInterop": true,
        "noEmit": true,
        "jsx": "react",
        "baseUrl": "./",
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "paths": {
            "utils": [
                "./src/utils/index.ts"
            ],
            "@/*": ["src/*"]
        }
    },
    "include": [
        "src",
        "./**/*.ts",
    ],
    "exclude": [
        "node_modules/**/*"
    ]
}